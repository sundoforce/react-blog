  }));\n    // 인풋 변경 인벤트 핸들러\n    const onChange = e => {\n        const { value, name } = e.target;\n        dispatch(\n            changeField({\n                form: 'register',\n                key: name,\n                value\n            })\n        )\n    };\n\n    // 폼 등록 이벤트 헨들러\n    const onSubmit = e => {\n        e.preventDefault();\n        const { username, password, passwordConfirm } = form;\n        if (password !== passwordConfirm) {\n            // TODO \n            return ;\n        }\n        dispatch(register({ username, password }));\n    }\n\n    // 컴포넌트가 처음 렌더링 될 때 form을 초기화 함\n    useEffect(()=> {\n        dispatch(initializeForm('register'));\n\n    }, [dispatch]);\n\n    // 휘원가입 성공/실채 처리\n    useEffect(() => {\n        if (authError) {\n            console.log(\"오류발생\");\n            console.log(authError);\n            return;\n        }\n        if (auth) {\n            console.log('회원가입 성공');\n            console.log(auth);\n            dispatch(check());\n        }\n    }, [auth, authError, dispatch]); \n\n\n    // user 값이 잘 설정 되었는지 확인\n    useEffect(()=>{\n        if (user) {\n            history.push('/'); // 혹화면으로 이동\n        }\n    }, [history, user])\n\n    return (\n        <AuthForm\n        type=\"register\"\n        form={form}\n        onChange={onChange}\n        onSubmit={onSubmit}\n        />\n    );\n};\n\nexport default RegisterForm;",["67","68"],"/Users/sdk/IdeaProjects/blog/blog-frontend/src/modules/user.js",["69","70","71","72"],"/Users/sdk/IdeaProjects/blog/blog-frontend/src/components/auth/AuthForm.js",[],"/Users/sdk/IdeaProjects/blog/blog-frontend/src/components/auth/AuthTemplate.js",[],"/Users/sdk/IdeaProjects/blog/blog-frontend/src/App.js",[],"/Users/sdk/IdeaProjects/blog/blog-frontend/src/pages/RegisterPage.js",[],"/Users/sdk/IdeaProjects/blog/blog-frontend/src/pages/PostPage.js",[],"/Users/sdk/IdeaProjects/blog/blog-frontend/src/modules/loading.js",[],"/Users/sdk/IdeaProjects/blog/blog-frontend/src/lib/api/a